{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@const/*": [
                "./src/constants/*"
            ],
            "@modules/*": [
                "./src/modules/*"
            ]
        }
    },
}